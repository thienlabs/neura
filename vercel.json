{
  "version": 2,
  "name": "neura-app",
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ],
  "buildCommand": "flutter build web --release --web-renderer html",
  "outputDirectory": "build/web"
}