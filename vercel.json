{
  "version": 2,
  "name": "neura-app",
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    },
    {
      "src": "/(.*)",
      "status": 200,
      "dest": "/build/web/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}